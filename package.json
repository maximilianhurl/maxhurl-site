{
  "name": "maxhurl-site",
  "version": "1.0.0",
  "description": "",
  "main": "static/js/canvas.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-less": "lessc static/src/less/styles.less static/build/css/styles.css && lessc static/src/less/print.less static/build/css/print.css",
    "watch-less": "watch 'npm run build-less' static/src/less/",
    "build-js": "browserify -t babelify static/src/js/canvas.js -o static/build/js/canvas.js",
    "build-js-min": "browserify -t babelify static/src/js/canvas.js -o static/build/js/canvas.js -p [minifyify --no-map]",
    "watch-js": "watchify -t babelify static/src/js/canvas.js -o static/build/js/canvas.js -v",
    "build-dev": "npm run build-less && npm run build-js",
    "build-prod": "npm run build-less && npm run build-js-min"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maximilianhurl/maxhurl-site.git"
  },
  "author": "Max Hurl",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/maximilianhurl/maxhurl-site/issues"
  },
  "homepage": "https://github.com/maximilianhurl/maxhurl-site#readme",
  "dependencies": {
    "babel-cli": "=6.3.17",
    "babel-preset-es2015": "=6.3.13",
    "babelify": "=7.2.0",
    "browserify": "=12.0.1",
    "jquery": "=3.4.1",
    "less": "=3.0.4",
    "matter-js": "=0.8.0",
    "minifyify": "=7.1.0",
    "poly-decomp": "=0.1.1",
    "underscore": "=1.8.3",
    "visibilityjs": "=1.2.1",
    "watch": "=0.17.1"
  },
  "devDependencies": {
    "watchify": "=3.6.1"
  }
}
